        tools:layout_editor_absoluteY="414dp" />

    <Button
        android:id="@+id/BotonSumarCroissant"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:text="+"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="346dp"
        tools:layout_editor_absoluteY="413dp" />

    <Button
        android:id="@+id/botonSumarTostada"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:text="+"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="346dp"
        tools:layout_editor_absoluteY="327dp" />

    <Button
        android:id="@+id/botonRestarTostada"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:text="-"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="214dp"
        tools:layout_editor_absoluteY="328dp" />

    <Button
        android:id="@+id/botonSumarInfusion"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:text="+"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="346dp"
        tools:layout_editor_absoluteY="231dp" />

    <Button
        android:id="@+id/botonRestarInfusion"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:text="-"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="214dp"
        tools:layout_editor_absoluteY="232dp" />

    <Button
        android:id="@+id/botonRestarCafe"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:text="-"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="214dp"
        tools:layout_editor_absoluteY="131dp" />

    <Button
        android:id="@+id/botonSumarCafe"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:text="+"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="346dp"
        tools:layout_editor_absoluteY="130dp" />

    <TextView
